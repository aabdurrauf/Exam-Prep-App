<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="270.0" prefWidth="340.0" style="-fx-background-color: #EAE0D5;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finalstudymanagementsystem.AddNewController">
   <children>
      <Label layoutX="108.0" layoutY="14.0" prefHeight="34.0" prefWidth="145.0" text="Add New Exam" textAlignment="CENTER">
         <font>
            <Font name="Gadugi Bold" size="20.0" />
         </font>
      </Label>
      <ComboBox fx:id="examType" layoutX="40.0" layoutY="63.0" prefHeight="26.0" prefWidth="118.0" promptText="Exam Type" style="-fx-background-color: #D9C5AE;">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ComboBox>
      <ComboBox fx:id="semester" layoutX="181.0" layoutY="63.0" prefHeight="26.0" prefWidth="118.0" promptText="Semester" style="-fx-background-color: #D9C5AE;">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ComboBox>
      <DatePicker fx:id="startDate" layoutX="40.0" layoutY="111.0" prefHeight="26.0" prefWidth="118.0" promptText="Start Date" style="-fx-background-color: #D9C5AE;" />
      <DatePicker fx:id="endDate" layoutX="181.0" layoutY="111.0" prefHeight="26.0" prefWidth="118.0" promptText="End Date" style="-fx-background-color: #D9C5AE;" />
      <Button defaultButton="true" layoutX="123.0" layoutY="216.0" mnemonicParsing="false" onAction="#addButton" prefHeight="26.0" prefWidth="81.0" style="-fx-background-color: #D9C5AE;" text="Add">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="216.0" layoutY="216.0" mnemonicParsing="false" onAction="#cancelButton" prefHeight="26.0" prefWidth="81.0" style="-fx-background-color: #D9C5AE;" text="Cancel">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TextField fx:id="description" layoutX="40.0" layoutY="169.0" prefHeight="26.0" prefWidth="258.0" />
      <Label layoutX="40.0" layoutY="149.0" prefHeight="18.0" prefWidth="71.0" text="Description" textFill="#666666">
         <font>
            <Font name="Gadugi" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
